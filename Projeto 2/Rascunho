
main.c:




#include <stdio.h>
#include<stdlib.h>
#include<string.h>



int main(int argc,char *argv[]){

int i=0;
char palavra[50];

while((scanf ("%s", palavra)) != EOF){

i++;

}

printf("%d\n",i);

return 0;
}
///////////////////////


makefile:


projeto:
gcc -c main.c
gcc -o main main.o
./main < teste.txt

